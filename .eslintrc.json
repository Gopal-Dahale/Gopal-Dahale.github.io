{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": 0,
    "react/forbid-prop-types": 0,
    "react/no-danger": 0,
    "prettier/prettier": [
      "error",
      {
        "bracketSpacing": true,
        "endOfLine": "auto",
        "jsxBracketSameLine": true,
        "printWidth": 100,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "none"
      }
    ]
  }
}
